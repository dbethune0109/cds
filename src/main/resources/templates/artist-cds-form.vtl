<h1>Add an album to $artist.getName()</h1>

<form action="/cds" method="post">
  <input id="artistId" name="artistId" type="hidden" value="$artist.getId()">

  <div id="cdm" class="form-group">
    <label for="description">Album Name</label>
    <input id="description" name="description" type="text" class="form-control">
  </div>

  <button type="submit" class="btn btn-success">Add Album</button>
</form>
