<h1>Cd Organizer!</h1>

#if ($artists)
  <ul>
    #foreach ($artist in $artists)
      <li><a href="/cds/$artist.getId()"> $artist.getName()</a></li>
      #foreach ($cd in $artist.getCds())
        <li> $cd.getDescription() </a></li>
      #end
    #end
  </ul>
#else
  <p>No Artists!</p>
#end


<a href="/cds/new">Add New CD</a>
